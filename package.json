{
  "name": "anmeldesystem-backend",
  "version": "1.4.2",
  "description": "",
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/sebamomann/anmeldesystem-backend.git"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build && cpx \"./src/templates/**/*\" \"./dist/src/templates\" && cpx \"./test/protractor/fixtures/**/*\" \"./dist/test/protractor/fixtures\"",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "cpx \"./src/templates/**/*\" \"./dist/src/templates\" && cpx \"./test/protractor/fixtures/**/*\" \"./dist/test/protractor/fixtures\" && nest start --watch ",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nest-modules/mailer": "^1.3.22",
    "@nestjs/common": "^7.0.13",
    "@nestjs/core": "^7.3.2",
    "@nestjs/jwt": "^6.1.2",
    "@nestjs/passport": "^6.2.0",
    "@nestjs/platform-express": "^6.11.11",
    "@nestjs/platform-socket.io": "^7.3.2",
    "@nestjs/typeorm": "^6.3.4",
    "@nestjs/websockets": "^7.3.2",
    "atob": "^2.1.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "latest",
    "btoa": "^1.2.1",
    "class-transformer": "^0.3.1",
    "connect-history-api-fallback": "^1.6.0",
    "date-fns": "^2.15.0",
    "dotenv": "^8.2.0",
    "ical-generator": "^1.10.0",
    "js-yaml": "^3.14.0",
    "mysql": "^2.18.1",
    "nestjs-easyconfig": "^1.8.10",
    "nodemailer": "^6.4.6",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.5",
    "socket.io": "^2.3.0",
    "typeorm": "^0.2.24",
    "web-push": "^3.4.4",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.11",
    "@nestjs/cli": "^6.14.2",
    "@nestjs/schematics": "^6.9.4",
    "@nestjs/testing": "^6.11.11",
    "@nestjs/typeorm": "^6.3.4",
    "@types/express": "^4.17.6",
    "@types/jest": "^24.9.1",
    "@types/node": "^12.12.37",
    "@types/passport-jwt": "^3.0.3",
    "@types/passport-local": "^1.0.33",
    "@types/socket.io": "^2.1.8",
    "@types/supertest": "^2.0.8",
    "cpx": "^1.5.0",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "supertest": "^4.0.2",
    "ts-jest": "^24.3.0",
    "ts-loader": "^6.2.2",
    "ts-node": "^8.9.0",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^5.20.0",
    "typeorm": "^0.2.24",
    "typescript": "^3.8.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "coveragePathIgnorePatterns": [
      ".entity.ts",
      ".decorator.ts",
      ".strategy.ts",
      "src/exceptions"
    ],
    "testEnvironment": "node"
  }
}
